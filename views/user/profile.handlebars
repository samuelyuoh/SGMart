<script>
    var button = document.getElementById('myButton');
    var x = document.getElementById("myButton").value;
    button.onclick = function() {
        var atLeastOneIsChecked = $('input[name="myButton"]:checked').length > 0;
        if (atLeastOneIsChecked) {
            location.assign('/user/'+x+'/2fa/enable');
        } else {
            location.assign('/user/'+x+'/2fa/disable');
        }      
    }
      var imagePreview = function(event) {
        var reader = new FileReader();
        reader.onload = function(){
          var output = document.getElementById('pfp');
          output.src = reader.result;
        };
        reader.readAsDataURL(event.target.files[0]);
      };
</script>
